(executable
  (name test))

(rule
  (action
  (with-outputs-to tests.output
   (chdir %{project_root}
   (run ocamlclose -r text -b list-only
        lib/typed.ml
        lib/close.ml
        lib/conf.ml
        test/open_scope.ml
        )))))

(rule
  (alias runtest)
  (action (diff tests.expected tests.output)))
