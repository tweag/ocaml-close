(root)

(rules

  (keep
     (or (in-list ("Base" "Core" "Core_kernel"))
         exports-syntax
         exports-modules-only
         (<= scope-lines 40)))

   (remove (and (<= uses 5) (<= name-length 15)))

   (structure (and (<= symbols 5) (>= uses 15)))

   (local (<= functions 4))

   (move (>= dist-to-optimal 40)))

(precedence (keep remove local structure move))
